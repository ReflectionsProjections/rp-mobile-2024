{
  "expo": {
    "scheme": "reflectionsprojections",
    "name": "rp-mobile-2024",
    "slug": "rp-mobile-2024",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow access to camera to help you scan into R|P 2024 events and collect points.",
          "recordAudioAndroid": false
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.reflectionsprojections.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "org.reflectionsprojections.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "updates": {
      "url": "https://u.expo.dev/d54e6f30-0fdc-427a-bab4-a1bad3a57fcf"
    },
    "owner": "rpdev2024",
    "extra": {
      "eas": {
        "projectId": "d54e6f30-0fdc-427a-bab4-a1bad3a57fcf"
      }
    }
  }
}
